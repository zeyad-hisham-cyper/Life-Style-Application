    height: 16px;
    border-right: 1px solid #19232D;
    image: url(&quot;:/qss_icons/rc/up_arrow.png&quot;);
  }
  
  /* QToolBox --------------------------------------------------------------
  
  https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox
  
  --------------------------------------------------------------------------- */
  QToolBox {
    padding: 0px;
    border: 1px solid #32414B;
  }
  
  QToolBox::selected {
    padding: 0px;
    border: 2px solid #1464A0;
  }
  
  QToolBox::tab {
    background-color: #19232D;
